                },
                "integer": {
                    "regex": /^[\-\+]?\d+$/,
                    "alertText": '<spring:message code="valid.integer.txt"/>'
                },
                "number": {
                    // Number, including positive, negative, and floating decimal. credit: orefalo
                    "regex": /^[\-\+]?(([0-9]+)([\.,]([0-9]+))?|([\.,]([0-9]+))?)$/,
                    "alertText": '<spring:message code="valid.number.txt"/>'
                },
                "date": {
                    "regex": /^\d{4}[\/\-](0?[1-9]|1[012])[\/\-](0?[1-9]|[12][0-9]|3[01])$/,
